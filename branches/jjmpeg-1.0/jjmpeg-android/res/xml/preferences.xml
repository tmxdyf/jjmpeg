<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">
	<PreferenceCategory
		android:title="Video Options"
		android:key="pref_video">
		<ListPreference
			android:key="pref_throttle"
			android:title="Drop Frames"
			android:summary="Fixed frame drop rate."
			android:entries="@array/throttle_labels"
			android:entryValues="@array/throttle_values"
			android:defaultValue="1" />
	</PreferenceCategory>
	<PreferenceCategory
		android:title="Developer Options"
		android:key="pref_developer">
		<CheckBoxPreference
			android:key="pref_debug"
			android:title="Debug Overlay"
			android:summary="Show various debug and profiling information."
			android:defaultValue="false" />
	</PreferenceCategory>
</PreferenceScreen>
