        <nativelib href = "jjmpeg-natives-linux-i586.jar" />
    </resources>
    <resources os="Linux" arch="amd64">
        <nativelib href = "jjmpeg-natives-linux-amd64.jar" />
    </resources>
    <resources os="Linux" arch="x86_64">
        <nativelib href = "jjmpeg-natives-linux-amd64.jar" />
    </resources>
    <resources os="Mac OS X" arch="i386">
        <nativelib href = "jjmpeg-natives-macosx-universal.jar" />
    </resources>
    <resources os="Mac OS X" arch="x86_64">
        <nativelib href = "jjmpeg-natives-macosx-universal.jar" />
    </resources>
